package com.school.Repository;

import org.springframework.data.jpa.repository.JpaRepository;
import org.springframework.data.jpa.repository.Query;

import com.school.Entity.Principal;

public interface Principalrepo extends JpaRepository<Principal, Integer> {

	
	@Query(value="select * from princiapl where email= :nm",nativeQuery=true)
	public Principal findbyusername(@Param("nm") String nm)
}
